#
# ADDITIONAL_FEATURE_PROPERTIES FOR DTGermany SPEC
#
####################################################
#Set the current spec as DTGermany#
persist.env.spec=DTGermany

#set system locale#
persist.sys.locale=de-DE

#set system timezone#
persist.sys.timezone=Europe/Berlin

#Set the SIM model for DSDS. ssss - Single model, dsds - Dual model.#
persist.radio.multisim.config=ssss

# Default for Settings.Global.DATA_ROAMING
ro.com.android.dataroaming=false
